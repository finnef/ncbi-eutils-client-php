{
  "name": "umnlib/ncbi-eutils-client",
  "description": "A PHP client package for the National Center for Biotechnology Information (NCBI) Entrez Programming Utilities (E-utilities).",
  "keywords": [],
  "authors": [
    {
      "name": "David Naughton",
      "email": "naughton@umn.edu"
    }
  ],

  "require": {
    "umnlib/argvalidator": "1.0.*",
    "umnlib/file-set-datesequence": "dev-master",
    "umnlib/set": "1.0.*",
    "guzzlehttp/guzzle": "^6.2.1"
  },

  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:UMNLibraries/argvalidator-php.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:finnef/file-set-datesequence-php.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:UMNLibraries/set-php.git"
    }
  ],   

  "require-dev": {
    "phpunit/phpunit": ">=3.7"
  },

  "autoload": {
    "psr-4": {"UmnLib\\Core\\NcbiEUtilsClient\\": "src/"}
  },

  "autoload-dev": {
    "psr-4": {"UmnLib\\Core\\Tests\\NcbiEUtilsClient\\": "tests/"}
  }
}
